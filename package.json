{
  "name": "necro-automobilia-service",
  "version": "0.2.0",
  "description": "Project host services related to traffic accident fatalities",
  "license": "MIT",
  "author": {
    "name": "Harlan Sang",
    "email": "harlan@linkedmink.space"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LinkedMink/necro-automobilia-service"
  },
  "private": true,
  "scripts": {
    "start": "ts-node-dev --inspect --respawn --transpileOnly -r dotenv/config ./src/App.ts",
    "build": "tsc --project ./tsconfig.json",
    "build:prod": "tsc --project ./tsconfig.prod.json",
    "postbuild": "node -r dotenv/config ./build/scripts/PostBuild.js",
    "postbuild:prod": "node -r dotenv/config ./build/scripts/PostBuild.js",
    "startApp": "node -r dotenv/config ./build/App.js",
    "watch": "ts-node-dev --respawn --transpileOnly -r dotenv/config ./src/App.ts",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src ./tests",
    "format": "prettier --write ./{src,tests}/**/*.ts docs/*.yaml ./*.js{,on}",
    "containerize": "npm run build && npm run postBuild && docker build -t linkedmink/necro-automobilia-service ."
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.0",
    "dotenv": "^8.2.0",
    "email-templates": "^8.0.3",
    "express": "^4.17.1",
    "mongoose": "^5.11.13",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pug": "^3.0.0",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/email-templates": "^8.0.1",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.17",
    "@types/jest": "^26.0.20",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.22",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.5",
    "@types/passport-jwt": "^3.0.3",
    "@types/pug": "^2.0.4",
    "@types/serve-static": "^1.13.9",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "openapi-types": "^7.2.3",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
